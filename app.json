{
  "expo": {
    "name": "SafarYatraAI",
    "slug": "safaryatraai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safaryatraai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.safaryatraai.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1A1C19"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safaryatraai.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7daf7f93-1ad5-4c8f-ad4b-442146e2d1ee"
      }
    }
  }
}